{
	"name": "raichu",
	"version": "1.0.0",
	"description": "React/Typescript/Electron based training data analysis application",
	"main": "./dist/main.bundle.js",
	"scripts": {
		"start": "electron ./dist/main.bundle.js",
		"dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --colors",
		"prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
		"test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
		"build:win": "electron-builder build --win --x64",
		"build:mac": "electron-builder build --mac --x64"
	},
	"build": {
		"appId": "jon-smith.raichu",
		"productName": "raichu",
		"directories": {
			"app": "./dist/",
			"output": "./out/"
		},
		"win": {
			"target": "nsis",
			"asar": false
		},
		"buildVersion": "1.0.0"
	},
	"author": "jon-smith",
	"license": "MIT",
	"dependencies": {
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"lodash": "^4.17.15"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^2.9.0",
		"@typescript-eslint/experimental-utils": "^2.9.0",
		"@typescript-eslint/parser": "^2.9.0",
		"@types/react": "^16.9.13",
		"@types/react-dom": "^16.9.4",
		"@types/lodash": "^4.14.149",
		"@types/mocha": "5.2.7",
		"acorn": "^7.1.0",
		"ajv": "^6.10.2",
		"copy-pkg-json-webpack-plugin": "0.0.38",
		"cross-env": "^6.0.3",
		"css-loader": "^3.2.0",
		"electron": "^7.1.2",
		"electron-builder": "^21.2.0",
		"eslint": "^6.7.1",
		"eslint-import-resolver-webpack": "^0.11.1",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.16.0",
		"eslint-plugin-react-hooks": "^2.3.0",
		"file-loader": "^2.0.0",
		"html-webpack-plugin": "^3.2.0",
		"mocha": "^6.2.2",
		"rimraf": "^3.0.0",
		"source-map-loader": "^0.2.4",
		"spectron": "^5.0.0",
		"style-loader": "^1.0.0",
		"ts-loader": "^6.2.1",
		"ts-node": "^7.0.1",
		"tsconfig-paths": "^3.7.0",
		"typescript": "^3.7.2",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jon-smith/raichu"
	},
	"keywords": [
		"Electron",
		"React",
		"Typescript",
		"Webpack"
	],
	"bugs": {
		"url": "https://github.com/jon-smith/raichu/issues"
	},
	"homepage": "https://github.com/jon-smith/raichu#readme"
}
