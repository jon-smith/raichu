{
	"name": "raichu",
	"version": "0.0.1",
	"description": "React/Typescript/Electron based training data analysis application",
	"main": "./dist/main.js",
	"scripts": {
		"build-native": "electron-build-env neon build jolteon --release",
		"build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
		"build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
		"build": "npm run build-main && npm run build-renderer",
		"start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
		"start-main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
		"start-dev": "cross-env START_HOT=1 npm run start-renderer-dev",
		"prestart": "npm run build",
		"start": "electron .",
		"lint": "eslint --ext=jsx,js,tsx,ts src",
		"test": "jest",
		"pretest:e2e": "npm run build",
		"test:e2e": "jest /test/e2e\\\\",
		"test:native": "jest /test/native\\\\",
		"pack": "npm run build && electron-builder --dir",
		"dist": "npm run build && electron-builder",
		"postinstall": "electron-builder install-app-deps"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"{src,test,mocks}/**/*.{json,css,scss,md}": [
			"prettier --config ./.prettierrc --write"
		],
		"{src,test,mocks}/**/*.{js,ts,tsx}": [
			"prettier --config ./.prettierrc --write",
			"eslint --ext=jsx,js,ts,tsx --fix src"
		]
	},
	"jest": {
		"projects": [
			{
				"displayName": "e2e tests",
				"transform": {
					"^.+\\.tsx?$": "ts-jest"
				},
				"testMatch": [
					"**/test/e2e/*.(spec|test).ts(|x)"
				],
				"moduleFileExtensions": [
					"ts",
					"tsx",
					"js",
					"json",
					"node"
				],
				"modulePaths": [
					"<rootDir>/src"
				],
				"moduleNameMapper": {
					"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
					"\\.(s?css|sass)$": "<rootDir>/mocks/styleMock.js"
				}
			},
			{
				"displayName": "Electron environment tests",
				"transform": {
					"^.+\\.tsx?$": "ts-jest"
				},
				"moduleFileExtensions": [
					"ts",
					"tsx",
					"js",
					"json",
					"node"
				],
				"modulePaths": [
					"<rootDir>/src"
				],
				"moduleNameMapper": {
					"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
					"\\.(s?css|sass)$": "<rootDir>/mocks/styleMock.js"
				},
				"runner": "@jest-runner/electron",
				"testEnvironment": "@jest-runner/electron/environment",
				"testMatch": [
					"**/test/!(e2e)/*.(spec|test).ts(|x)"
				]
			}
		]
	},
	"build": {
		"productName": "raichu",
		"appId": "jon-smith.raichu",
		"mac": {
			"category": "fitness"
		},
		"directories": {
			"output": "release"
		},
		"files": [
			"dist/",
			"node_modules/",
			"package.json"
		],
		"linux": {
			"target": "deb"
		},
		"win": {
			"target": "nsis"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jon-smith/raichu"
	},
	"author": {
		"name": "jon-smith"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jon-smith/raichu/issues"
	},
	"homepage": "https://github.com/jon-smith/raichu#readme",
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/polyfill": "^7.4.4",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.3.3",
		"@hot-loader/react-dom": "^16.8.6",
		"@jest-runner/electron": "^2.0.3",
		"@types/d3": "^5.7.2",
		"@types/electron-devtools-installer": "^2.2.0",
		"@types/jest": "^25.1.4",
		"@types/lodash": "^4.14.149",
		"@types/material-ui": "^0.21.7",
		"@types/react": "^16.9.25",
		"@types/react-dom": "^16.9.5",
		"@types/react-redux": "^7.1.7",
		"@types/react-resize-detector": "^4.2.0",
		"@types/react-test-renderer": "^16.8.1",
		"@types/webdriverio": "^4.8.7",
		"@types/webpack-env": "^1.13.3",
		"@typescript-eslint/eslint-plugin": "^2.4.0",
		"@typescript-eslint/parser": "^2.4.0",
		"babel-loader": "^8.0.6",
		"cross-env": "^5.1.3",
		"css-loader": "^2.1.1",
		"electron": "^7.0.0",
		"electron-build-env": "^0.2.0",
		"electron-builder": "^22.3.2",
		"electron-devtools-installer": "^2.2.4",
		"eslint": "^6.5.1",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-prettier": "^6.4.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react": "^7.16.0",
		"eslint-plugin-react-hooks": "^1.7.0",
		"file-loader": "^3.0.1",
		"fork-ts-checker-webpack-plugin": "^1.3.4",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^4.2.1",
		"image-webpack-loader": "^4.6.0",
		"jest": "^25.1.0",
		"lint-staged": "^10.0.7",
		"neon-cli": "^0.4.0",
		"node-loader": "^0.6.0",
		"node-sass": "^4.12.0",
		"prettier": "^1.18.2",
		"react-hot-loader": "^4.8.8",
		"react-test-renderer": "^16.8.6",
		"redux-devtools-extension": "^2.13.5",
		"sass-loader": "^7.1.0",
		"source-map-loader": "^0.2.4",
		"spectron": "^9.0.0",
		"style-loader": "^0.23.1",
		"ts-jest": "^25.2.1",
		"tsconfig-paths-webpack-plugin": "^3.2.0",
		"typescript": "^3.8.3",
		"webpack": "^4.32.2",
		"webpack-cli": "^3.3.2",
		"webpack-dev-server": "^3.4.1",
		"webpack-merge": "^4.2.1"
	},
	"dependencies": {
		"@material-ui/core": "^4.9.7",
		"@material-ui/icons": "^4.9.1",
		"@reduxjs/toolkit": "^1.3.5",
		"d3": "^5.15.1",
		"geolib": "^3.2.1",
		"jolteon": "file:native",
		"lodash": "^4.17.15",
		"rc-time-picker": "^3.7.3",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-dropzone": "^10.2.1",
		"react-redux": "^7.2.0",
		"react-resize-detector": "^4.2.1",
		"react-vis": "^1.11.7",
		"redux": "^4.0.5",
		"redux-batched-actions": "^0.5.0",
		"reselect": "^4.0.0"
	}
}
